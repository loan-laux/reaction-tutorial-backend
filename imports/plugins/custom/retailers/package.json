            "@babel/plugin-proposal-class-properties",
            {
              "loose": false
            }
          ],
          "@babel/plugin-proposal-json-strings",
          [
            "module-resolver",
            {
              "root": [
                "./"
              ],
              "alias": {
                "@reactioncommerce/reaction-collections": "./imports/plugins/core/collections",
